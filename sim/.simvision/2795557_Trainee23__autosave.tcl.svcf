#
# Waveform windows
#
if {[catch {window new WaveWindow -name "Waveform 1" -geometry 1398x540+51+59}] != ""} {
    window geometry "Waveform 1" 1398x540+51+59
}
window target "Waveform 1" on
waveform using {Waveform 1}
waveform sidebar select designbrowser
waveform set \
    -primarycursor TimeA \
    -signalnames name \
    -signalwidth 175 \
    -units ns \
    -valuewidth 75
waveform baseline set -time 0


waveform xview limits 0 1000000000ns

